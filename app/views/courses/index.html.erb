<% if signed_in? %>
  <div class='action'>
    <%= link_to 'Add a course', new_course_path %>
  </div>
<% end %>

<div class='subheading'><%= t '.courses_completed' %>:</div>
<% courses.each do |course| %>
  <section class='course'>
    <%= render 'course', course: course %>
  </section>
<% end %>

<%= render 'nav_home' %>